                <TableColumn fx:id="tcPersNr" prefWidth="75.0" text="PersNr"/>
                <TableColumn fx:id="tcVorname" prefWidth="107.0" text="Vorname"/>
                <TableColumn fx:id="tcNachname" prefWidth="84.0" text="Nachname"/>
